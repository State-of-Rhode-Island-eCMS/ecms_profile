{
  "name": "rhodeislandecms/ecms_profile",
  "description": "Drupal installation profile for the State of Rhode Island's eCMS system",
  "type": "drupal-profile",
  "authors": [
    {
      "name": "Oomph, Inc.",
      "email": "devops@oomphinc.com",
      "homepage": "https://www.oomphinc.com/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GPL-2.0-or-later",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "installer-types": ["bower-asset", "npm-asset"],
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "patches": {},
    "preserve-paths": [],
    "drupal-scaffold": {}
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "develop": "scripts/develop.sh",
    "clean-config": "scripts/clean-config.sh",
    "post-package-install": [
      "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    ],
    "post-package-update": [
      "Drupal\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    ]
  },
  "provide": {},
  "require": {
    "composer/installers": "^1.9",
    "drupal/core-composer-scaffold": "^9",
    "drupal/core-project-message": "^9",
    "drupal/core-recommended": "^9",
    "drupal/core-vendor-hardening": "^9",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/preserve-paths": "^0.1.4",
    "drush/drush": "^10.0",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/phpunit-bridge": "^5.1"
  }
}